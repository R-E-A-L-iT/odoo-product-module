<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ba_livechat_channel_view_inherit" model="ir.ui.view">
        <field name="name">ba.livechat.channel.view.form.inherit</field>
        <field name="model">im_livechat.channel</field>
        <field name="inherit_id" ref="im_livechat.im_livechat_channel_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Assignment Sequence">
                    <field name="user_sequences" nolabel="1">
                        <tree string="Assignment Sequence" editable="bottom">
                            <field name="sequence" widget="handle"/>
                            <field name="name" widget="many2one_avatar_user" required="1"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>

</odoo>