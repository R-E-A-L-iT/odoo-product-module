<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="delivery_slip_backend" model="ir.ui.view">
        <field name="name">Delivery Slip Backend (PO#)</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.stock_picking_only_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name=&quot;origin&quot;]" position="after">
                <field name="po_number" invisible="0" />
            </xpath>
        </field>
    </record>
</odoo>