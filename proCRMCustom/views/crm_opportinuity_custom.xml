<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="crm_lead_opportinuity_custom" model="ir.ui.view">
        <field name="name">CRM_Lead_Backend_Form</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="opportunity_custom_status" />
                <field name="opportunity_sn" />
                <field name="opportunity_notes" />
                <field name="opportunity_log_with_leica" />
            </xpath>
        </field>
    </record>
</odoo>