<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="crm_lead_opportunity_custom_real" model="ir.ui.view">
        <field name="name">CRM_Lead_Backend_Form_Real</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tag_ids']" position="after">
                <field name="opportunity_custom_status" />
                <field name="opportunity_sn" />
                <field name="opportunity_notes" />
                <field name="linkedin_link" />
            </xpath>
        </field>
    </record>
</odoo>